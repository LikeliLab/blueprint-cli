{
  "name": "blueprint-cli",
  "version": "1.0.0",
  "description": "A simple CLI to generate files from EJS templates.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "blueprint": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "prepack": "npm run build"
  },
  "keywords": [
    "cli",
    "ejs",
    "templating",
    "node",
    "typescript"
  ],
  "author": "Michael Ellis",
  "license": "Apache-2.0",
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "ejs": "^3.1.10",
    "ora": "^9.0.0",
    "sanitize-filename": "^1.6.3"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^24.9.1",
    "@types/sanitize-filename": "^1.1.28",
    "typescript": "^5.9.3"
  }
}
